import java.util.*;

public class BMI {
	
public static void main(String args[]) {
		
		Scanner in = new Scanner(System.in);
		System.out.print(" enter your weight in kg: "); 
		double weight = in.nextDouble(); // weight in kg
		System.out.print(" enter your height in cm: ");
		double height = in.nextDouble(); // height in cm
		
		double bmi = (weight/height/height)*10000;
		System.out.print(bmi);
		
		if (bmi  <18.5 ) System.out.print(" Weight class: underweight");
		else if (bmi <25) System.out.print(" Weight class: normal");
		else if (bmi <30 ) System.out.print(" Weight class: overweight");
		else  System.out.print(" Weight class: obese");

	}
}
